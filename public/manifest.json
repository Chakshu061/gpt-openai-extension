{
  "manifest_version": 3,
  "name": "GPT-3 ask",
  "description": "Desc",
  "version": "1.0.0",
  "action": {
      "default_popup": "index.html",
      "default_title": "Open the popup"
  },
  "icons": {
      "16": "favicon-16x16.png",
      "32": "favicon-32x32.png",
      "192": "android-chrome-192x192.png",
      "512": "android-chrome-512x512.png"
  },
  "permissions": [
      "scripting",
      "activeTab",
      "storage"
  ],
  "host_permissions": [
      "https://*/"
  ]
}